{
  "version": "1",
  "pipeline": "machine_learning",
  "source": "inertial",
  "process": "none",
  "input": {
    "ws": "ws://url:8080/ws"
  },
  "windowing": {
    "enable": true,
    "filename": "output.csv",
    "parameters": {
      "length": "60Hz",
      "function": {
        "label": "none",
        "container": "core-window-functions"
      },
      "overlap": 0
    }
  },
  "features": {
    "save": true,
    "list": [
      {
        "label": "average",
        "container": "core-features"
      },
      {
        "label": "std_dev",
        "container": "core-features"
      }
    ]
  }
}
